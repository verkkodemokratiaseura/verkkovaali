<?xml version="1.0" encoding="UTF-8"?>
<xrds:XRDS
    meta:interpolation="false"
    xmlns="xri://$xrd*($v*2.0)"
    xmlns:xrds="xri://$xrds"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:meta="http://xml.zope.org/namespaces/meta">
  <XRD meta:interpolation="true">
    <Service priority="0">
      <tal:services repeat="service service_types">
      <Type tal:content="service"></Type>
      </tal:services>
      <URI>${uri}</URI>
      <LocalID tal:condition="local_id|None">${local_id}</LocalID>
    </Service>
  </XRD>
</xrds:XRDS>
