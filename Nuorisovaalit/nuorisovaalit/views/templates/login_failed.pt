<html xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      metal:use-macro="main.macros['master']">
  <head></head>
  <body>
    <metal:header fill-slot="header">
      <div id="failed-login">
        <h1 class="visuallyhidden">Nuorisovaalit 2011</h1>
      </div>
    </metal:header>

    <div id="openidlogin-failed"
         metal:fill-slot="main">

      <h2>Tunnistautuminen epäonnistui</h2>

      <p>${message}</p>

      <p>
        <a href="${request.application_url}">Yritä tunnistautumista
        uudelleen</a> tai ota tarvittaessa yhteyttä
        tekniseen tukipalveluun osoitteessa
        <a href="mailto:xxxx@xxxx.xx">xxxx@xxxx.xx</a>.
      </p>

    </div>
  </body>
</html>
